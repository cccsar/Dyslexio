.ast 1+2+3
.ast 1 - 2 - 3
.ast 1+2-3
.ast 1 - 2 + 3

.ast --1
.ast --1---2

.ast + + 1
.ast ++1+++2
.ast + + 1 + - - 2

.ast 1 * 2 * 3
.ast 1%2%3
.ast 1 * 2 % 3
.ast 1%2*3

.ast - - 1 * - - 2
.ast ++1*--2
.ast - - 1 % + + 2
.ast ++1%--2

.ast 1 + 2 * 3 - 4 % 5
.ast 1-2%3+4*5 

.ast 1 ^ 2 ^ 3
.ast 1^2+3
.ast 1 ^ 2 - 3
.ast 1^2*3
.ast 1 * 2 ^ 3
.ast 1%2^3
.ast 1 ^ 2 % 3

.ast 1 + 2 - 3 * 4 % 5 ^ 6
.ast 1^2%3*4-5+6

.ast 1 + 2 - --3 * ++4 % 5 ^ 6 ^ 7 ^ --8
